#include<stdio.h>
#include<conio.h>
int factorial(int); 
int print_fact(int,int);

 int main()
{
int j,t;
clrscr();

printf("Enter the numbers of terms - ");
scanf("d",&t);

for(j=1;j<=t;j++)
{
factorial(j);
}

getch();
}

int factorial (int num)
{
int fact, i;
fact = 1;
i =1;
while(i<=num)
{
fact *= i;
i++;
}
printf_fact(num,fact);
}

int print_fact(int n, int f)
{
printf("__________________");
printf("Number   |   Factorial   ");
printf("     %d     |         %d     ",n,f);
}
